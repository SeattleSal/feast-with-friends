{
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true
    },
    "rules": {
        "no-duplicate-case":"warning",
        "no-empty":"warning",
        "no-extra-semi":"warning",
        "no-func-assign": "warning",
        "no-irregular-whitespace":"warning",
        "no-unreachable": "warning",
        "curly": "warning",
        "dot-notation": "warning",
        "eqeqeq": "warning",
        "no-empty-function": "warning",
        "no-multi-spaces": "warning",
        "no-mixed-spaces-and-tabs": "warning",
        "no-trailing-spaces": "warning",
        "default-case": "warning",
        "no-fallthrough": "warning",
        "no-unused-vars": "warning",
        "no-use-before-define": "warning",
        "no-redeclare": "warning",
        "camelcase": "warning",
        "brace-style": "warning",
        "indent": [
            "warning",
            2
        ],
        "linebreak-style": [
            "warning",
            "unix"
        ],
        "quotes": [
            "warning",
            "double"
        ],
        "semi": [
            "warning",
            "always"
        ]
    }
  }
  